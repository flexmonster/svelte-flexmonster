{
	"name": "svelte-flexmonster",
	"version": "2.9.121",
	"license": "https://www.flexmonster.com/software-license-agreement/",
	"description": "Svelte wrapper for Flexmonster Pivot Table and Charts",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint"
	},
	"keywords": [
		"pivot",
		"html5",
		"grid",
		"table",
		"pivottable",
		"pivotgrid",
		"pivotchart",
		"svelte"
	],
	"author": {
		"name": "Flexmonster",
		"email": "help@flexmonster.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/flexmonster/svelte-flexmonster.git"
	},
	"bugs": {
		"url": "https://flexmonster.com/technical-support"
	},
	"homepage": "https://flexmonster.com",
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0",
		"flexmonster": "2.9.121"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"typescript": "^5.3.2",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"flexmonster": "2.9.121"
	}
}
